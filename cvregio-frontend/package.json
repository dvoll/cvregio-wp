    "react-shadow-dom-retarget-events": "^1.0.11",
    "react-transition-group": "^4.3.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "swiper": "^5.3.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-merge": "^4.2.2"
