{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "ev-region22-blocks/layout-card-grid",
	"version": "0.1.0",
	"title": "Card Grid Layout",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"align": true,
		"html": false
	},
	"textdomain": "layout-card-grid",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"postsToShow": {
			"type": "number",
			"default": 3
		},
		"categoryId": {
			"type": "number",
			"default": -1
		},
		"thumbnailSizeSlug": {
			"type": "string",
			"default": "thumbnail"
		}
	}
}
